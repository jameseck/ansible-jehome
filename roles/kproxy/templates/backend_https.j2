{% for k,v in (hosts_map | from_json).items() %}
{% if v.ansible_playbook == 'kubeadm.yml' %}
backend bk_https_{{ k | replace('.', '') }}
  mode tcp
  server {{ k }} {{ v.ip }}:443 check port 443
{% endif %}
{% endfor %}

{% for k,v in haproxy_ends.items() %}
backend bk_https_{{ k | replace('.', '') }}
  mode tcp
{% for ip in v.ips %}
  server {{ k }} {{ ip }}:443 check port 443
{% endfor %}
{% endfor %}
